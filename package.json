{
  "name": "todoapp",
  "description": "",
  "homepage": "https://arnabk.github.io/todoapp",
  "dependencies": {
    "immutable": "^3.8.1",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "lokijs": "^1.5.0",
    "material-ui": "^0.19.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.0.13",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import-asserts": "^3.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "caniuse-api": "^2.0.0",
    "chai": "^4.1.1",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^3.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^20.0.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "http-server": "^0.10.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.0",
    "node-sass": "^4.2.0",
    "npm-check-updates": "^2.12.1",
    "nyc": "^11.2.1",
    "parse-json": "^3.0.0",
    "sass-loader": "^6.0.6",
    "scss": "^0.2.4",
    "source-map": "^0.5.6",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "v3.5.5",
    "yarn": "^1.0.2"
  },
  "version": "0.1.0",
  "engines": {
    "node": "~v7.10.0"
  },
  "author": "Arnab Karmakar",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --all mocha --recursive src/**/__tests__/**",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "watch": "webpack --progress --colors --watch -d --devtool source-map --config ./webpack.dev.js",
    "build": "webpack --progress --colors -p --config ./webpack.prod.js",
    "start": "concurrently -k \"yarn watch\" \"http-server dist\"",
    "update-pkg-version": "ncu -u -x eslint,eslint-config-airbnb,eslint-plugin-import,eslint-plugin-jsx-a11y,eslint-plugin-react"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "typecheck",
      "transform-decorators-legacy",
      "transform-class-properties",
      "add-module-exports",
      "import-asserts",
      "syntax-async-functions",
      "transform-regenerator"
    ],
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-core/register",
      "babel-polyfill",
      "mocha"
    ],
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/__tests__/*.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "html",
      "text",
      "text-lcov"
    ]
  }
}
